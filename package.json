    "url": "https://github.com/kentcdodds/js-mocking-fundamentals/issues"
  },
  "homepage": "https://github.com/kentcdodds/js-mocking-fundamentals#readme",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
